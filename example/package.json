{
  "name": "exify-example",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "clean": "npx expo prebuild --clean",
    "doctor": "npx expo-doctor",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "typecheck": "tsc --noEmit -p . --pretty",
    "tidy": "prettier --write \"**/*.{js,json,ts,tsx}\""
  },
  "dependencies": {
    "expo": "~50.0.11",
    "expo-status-bar": "~1.11.1",
    "react": "18.2.0",
    "react-native": "0.73.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/metro-config": "^0.73.5",
    "@react-native/typescript-config": "^0.74.0",
    "@types/react": "~18.2.45",
    "babel-plugin-module-resolver": "^5.0.0",
    "prettier": "^3.2.5",
    "typescript": "^5.1.3"
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "engines": {
    "node": ">=18"
  }
}
